{
  "manifest_version": 2,

  "name": "InterestMe",
  "description": "InterestMe Chrome Extension for Saving Articles",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup",
    "default_icon" : "icon.png"
  },

  "permissions": [
    "tabs",
    "activeTab",
    "https://interest-me-web.herokuapp.com/"
  ]
}
